<view><view data-event-opts="{{[['touchstart',[['dealTouchstart',['$event']]]],['touchend',[['dealTouchend',['$event']]]]]}}" class="videoBox" bindtouchstart="__e" bindtouchend="__e"><video src="{{item.video_src}}" id="myvideo" objectFit="fill" controls="{{false}}" show-play-btn="{{false}}" autoplay="true" data-event-opts="{{[['play',[['e0',['$event']]]]]}}" bindplay="__e"><cover-image class="icon" src="../static/images/message.png" data-event-opts="{{[['tap',[['gotoMessage',['$event']]]]]}}" bindtap="__e"></cover-image><cover-view class="number">{{item.comment_times}}</cover-view><block wx:if="{{item.appreciateFlag!='0'}}"><cover-image class="icon" src="../static/images/hearta.png" data-event-opts="{{[['tap',[['dealheart']]]]}}" bindtap="__e"></cover-image></block><block wx:else><cover-image class="icon" src="../static/images/heart.png" data-event-opts="{{[['tap',[['dealheart']]]]}}" bindtap="__e"></cover-image></block><cover-view class="number">{{item.star_times}}</cover-view><cover-view class="describeContent">{{item.description}}</cover-view></video></view><view class="status_bar"><view class="top_view"></view></view><view class="header clearfix"><view class="imgBox"><image src="{{item.user_picture}}"></image><text></text><text class="name">{{item.nickName}}</text></view><view class="focusBox"><block wx:if="{{item.followFlag==='0'}}"><text data-event-opts="{{[['tap',[['dealFocus']]]]}}" class="focus" bindtap="__e">关注</text></block><block wx:else><text data-event-opts="{{[['tap',[['dealFocus']]]]}}" class="focus" bindtap="__e">取消关注</text></block></view></view><footer-nav vue-id="1" bind:__l="__l"></footer-nav></view>