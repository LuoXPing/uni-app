.clearfix::before,
.clearfix::after{
	content: '';
	display: block;
	clear: both;
}
.status_bar {  
    height: var(--status-bar-height);  
    width: 100%;  
    background-color: #F8F8F8;
}
.top_view {  
    height: var(--status-bar-height);  
    width: 100%;  
    position: fixed;  
    background-color: #F8F8F8;  
    top: 0;  
    z-index: 999;
}

@font-face {
	font-family: "iconfont";
	/* //at.alicdn.com/t/font_1434292_ajt9704xq4w.ttf */
