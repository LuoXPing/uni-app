<view class="content"><view class="inputBox"><input type="text" placeholder="请输入昵称" placeholder-style="color:#747888" data-event-opts="{{[['input',[['__set_model',['','nickName','$event',[]]]]]]}}" value="{{nickName}}" bindinput="__e"/><input type="text" placeholder="请输入账号" placeholder-style="color:#747888" data-event-opts="{{[['input',[['__set_model',['','account','$event',[]]]]]]}}" value="{{account}}" bindinput="__e"/><input type="password" placeholder="请输入密码" placeholder-style="color:#747888" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><input type="password" placeholder="请确认密码" placeholder-style="color:#747888" data-event-opts="{{[['input',[['__set_model',['','rePassword','$event',[]]]]]]}}" value="{{rePassword}}" bindinput="__e"/><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><radio style="transform:scale(0.7);" value="男" checked="{{sexChecked===0}}" color="#0d6fb8"></radio><label class="iconfont icon-nan"></label><radio class="radioBtn" style="transform:scale(0.7);" value="女" checked="{{sexChecked===1}}" color="#0d6fb8"></radio><label class="iconfont icon-nv"></label></radio-group></view><button data-event-opts="{{[['tap',[['dealregister',['$event']]]]]}}" class="registbtn" bindtap="__e">注册</button><navigator class="registerWord" url="login">返回登录</navigator></view>